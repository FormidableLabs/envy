{
  "name": "@envyjs/example-apollo-client",
  "version": "1.0.0",
  "description": "Example website connected to apollo server application using Envy",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "concurrently \"yarn start:envy\" \"yarn start:web\" \"yarn start:viewer\"",
    "start:web": "parcel ./src/index.html --port 4001 --no-cache",
    "start:envy": "npx @envyjs/webui --noUi",
    "start:viewer": "parcel ./src/viewer/viewer.html --port 4002 --no-cache"
  },
  "dependencies": {
    "@envyjs/web": "*",
    "@envyjs/webui": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "urql": "^4.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "parcel": "^2.9.3",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}
